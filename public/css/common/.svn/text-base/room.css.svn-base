#room{width:100%;height:100%;background-repeat:no-repeat;background-size:cover;z-index:102}
#loading{position:absolute;width:100%;height:100%;color:#fff;background:#636363;font-size:12px;text-align:center;z-index:200;}
#loading .main{position:absolute;top:30%;width:100%;height:90px}
#error{position:absolute;width:100%;height:100%;background:#eee;text-align:center;line-height:4;z-index:200}
#room[data-theme="001"][data-time="noon"]{background-image:url("../../img/room/bg_001_noon.png")}
